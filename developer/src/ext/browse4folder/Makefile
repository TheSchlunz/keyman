#
# Keyman root Makefile
#

!include ..\..\Defines.mak

# ----------------------------------------------------------------------

build: dirs compile install

compile:
    @-mkdir obj\Win32\$(TARGET_PATH)
    $(DCC32DPK) browse4folder.dpk

install:
    $(DEVTOOLS) -ip $(OUTLIB)\browse4folder.bpl

# ----------------------------------------------------------------------
# clean target
# ----------------------------------------------------------------------

clean: def-clean
    $(CLEAN) *.bpl

# ----------------------------------------------------------------------

!include ..\..\Target.mak

# ----------------------------------------------------------------------
# EOF
# ----------------------------------------------------------------------
