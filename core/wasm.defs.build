[binaries]
c = ['emcc.py', '-s', 'WASM=1', '-O2']
cpp = ['em++.py', '-s', 'WASM=1','-O2']
ar = ['emar.py']
exe_wrapper = 'node'

[properties]
shared_lib_suffix = 'js'
static_lib_suffix = 'js'
shared_module_suffix = 'js'
exe_suffix = 'js'

[host_machine]
system = 'emscripten'
cpu_family = 'x86'
cpu = 'i686'
endian = 'little'
